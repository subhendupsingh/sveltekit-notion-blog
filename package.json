{
	"name": "sveltekit-notion-blog",
	"version": "0.1.13",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"patch": "npm version patch",
		"minor": "npm version minor"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.2.12"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-netlify": "^3.0.0",
		"@sveltejs/adapter-vercel": "^4.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.43.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.9",
		"svelte": "^4.2.12",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.3",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.0"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.19",
		"@notionhq/client": "^2.2.15",
		"@unpic/svelte": "^0.0.17",
		"mode-watcher": "^0.3.1",
		"neverthrow": "^6.0.0",
		"svelte-meta-tags": "^2.8.0"
	},
	"homepage": "https://sveltekit-notion-blog.netlify.app/",
	"description": "A plug and play library for setting up blogs in subdirectory or main directory in Sveltekit projects using Notion as CMS.",
	"keywords": [
		"sveltekit",
		"notion",
		"cms",
		"blog",
		"svelte",
		"sveltejs",
		"typescript",
		"javascript"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/subhendupsingh/sveltekit-notion-blog"
	},
	"engines": {
		"node": ">=18.13"
	}
}
